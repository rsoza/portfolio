service cloud.firestore {
  match /databases/{database}/documents {
    match /gifs/{document=**} {
      allow read: if true;
      allow write: if false;
    }
    match /work/{document=**} {
      allow read: if true;
      allow write: if false;
    }
    match /school/{document=**} {
      allow read: if true;
      allow write: if false;
    }
  }
}